program()
{
	println("TEST2 - CYCLES");
	println("##############");
	println("");

	println("The first cycle from 0 to 10");

	forcycle(integer i = 0, i <= 10, i++)
	{
		
		println(i);
	}

	println("");
	println("The second cycle from 10 to 0");

	forcycle(integer j = 10, j >= 0, j--)
	{
		println(j);
	}
}